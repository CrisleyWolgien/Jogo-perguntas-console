        elif pergunta_selecionada_k == "opcoes":
            for opcao_k, opcao_v in pergunta_selecionada_v.items():
                id_opcao = {int(k): v for k, v in perguntas.items()}
                if opcao_k == 1:
                    opcao_1 = opcao_v
                elif opcao_k == 2:
                    opcao_2 = opcao_v
                elif opcao_k == 3:
                    opcao_3 = opcao_v
                else:
                    opcao_4 = opcao_v